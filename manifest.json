{
  "manifest_version": 3,
  "name": "Text Search Extension",
  "version": "1.0",
  "description": "Search for specific text in new tabs",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "notifications"
  ],
  "action": {
    "default_title": "Open New Tab",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]    
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newTabOverride.html"
  }
}
