{
  "manifest_version": 3,
  "name": "New Tab Opener",
  "version": "1.0",
  "description": "Open a new tab in the current browser",
  "permissions": ["activeTab"],
  "action": {
    "default_title": "Open New Tab",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newTabOverride.html"
  }
}
